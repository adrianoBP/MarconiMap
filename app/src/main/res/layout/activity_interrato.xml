<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.heysidisi.marconimap.marconimap.interrato">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/layInterrato"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imgInterrato"
            android:layout_width="500dp"
            android:layout_height="600dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.353"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.842"
            app:srcCompat="@drawable/interrato" />

        <ImageView
            android:id="@+id/IL08IN21"
            android:layout_width="60dp"
            android:layout_height="5dp"
            app:layout_constraintRight_toLeftOf="@+id/IN20IN21"
            app:layout_constraintTop_toTopOf="@+id/IN20IN21"
            app:srcCompat="@color/colorPrimary" />

        <ImageView
            android:id="@+id/IN20INTR"
            android:layout_width="60dp"
            android:layout_height="5dp"
            android:layout_marginEnd="152dp"
            app:layout_constraintLeft_toRightOf="@+id/IN20IN21"
            app:srcCompat="@color/colorPrimary"
            app:layout_constraintBottom_toBottomOf="@+id/IN20IN21"
            android:layout_marginBottom="0dp" />

        <ImageView
            android:id="@+id/IN21IL01"
            android:layout_width="60dp"
            android:layout_height="5dp"
            android:layout_marginEnd="152dp"
            app:layout_constraintLeft_toRightOf="@+id/IN20IN21"
            app:layout_constraintTop_toTopOf="@+id/IN20IN21"
            app:srcCompat="@color/colorPrimary" />

        <ImageView
            android:id="@+id/IN20IN21"
            android:layout_width="5dp"
            android:layout_height="85dp"
            android:layout_marginBottom="113dp"
            android:layout_marginEnd="286dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="288dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imgInterrato"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintLeft_toLeftOf="@+id/imgInterrato"
            app:layout_constraintRight_toRightOf="@+id/imgInterrato"
            app:layout_constraintTop_toTopOf="@+id/imgInterrato"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@color/colorPrimary" />

        <ImageView
            android:id="@+id/IL03IN22"
            android:layout_width="60dp"
            android:layout_height="5dp"
            android:layout_marginEnd="131dp"
            app:layout_constraintBottom_toTopOf="@+id/IN22IN21"
            app:layout_constraintLeft_toRightOf="@+id/IL00IN22"
            app:srcCompat="@color/colorPrimary"
            android:layout_marginBottom="0dp" />

        <ImageView
            android:id="@+id/IN22IN21"
            android:layout_width="5dp"
            android:layout_height="170dp"
            app:layout_constraintBottom_toTopOf="@+id/IN20IN21"
            app:layout_constraintRight_toRightOf="@+id/IN20IN21"
            app:srcCompat="@color/colorPrimary" />

        <ImageView
            android:id="@+id/IL00IN22"
            android:layout_width="60dp"
            android:layout_height="5dp"
            app:layout_constraintRight_toLeftOf="@+id/IN22IN21"
            app:layout_constraintTop_toTopOf="@+id/IN22IN21"
            app:srcCompat="@color/colorPrimary"
            android:layout_marginRight="0dp" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/floatingActionButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:clickable="true"
            app:backgroundTint="@color/colorPrimary"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:srcCompat="?android:attr/actionModeWebSearchDrawable" />

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>
